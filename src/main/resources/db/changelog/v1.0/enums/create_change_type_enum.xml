<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1.1" author="SKamagurov">
        <sql>
            DROP TABLE IF EXISTS change_type CASCADE;
        </sql>
        <createTable tableName="change_type">
            <column name="change_type" type="varchar(50)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
        </createTable>
        <insert tableName="change_type">
            <column name="change_type" value="AUTOMATIC"/>
        </insert>
        <insert tableName="change_type">
            <column name="change_type" value="MANUAL"/>
        </insert>
    </changeSet>
</databaseChangeLog>