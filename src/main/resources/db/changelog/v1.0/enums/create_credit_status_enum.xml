<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1.2" author="SKamagurov">
        <sql>
            DROP TABLE IF EXISTS credit_status CASCADE;
        </sql>
        <createTable tableName="credit_status">
            <column name="credit_status" type="varchar(50)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
        </createTable>
        <insert tableName="credit_status">
            <column name="credit_status" value="CALCULATED"/>
        </insert>
        <insert tableName="credit_status">
            <column name="credit_status" value="ISSUED"/>
        </insert>
    </changeSet>
</databaseChangeLog>